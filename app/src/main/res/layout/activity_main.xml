<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="ICT2207 - EnsRingRing"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/jootingbutton"
        android:layout_width="360dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:text="CHIAM JOO TING"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title" />

    <Button
        android:id="@+id/zoiebutton"
        android:layout_width="360dp"
        android:layout_height="52dp"
        android:layout_marginTop="10dp"
        android:text="ZOIE CHEW"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/jootingbutton" />

    <Button
        android:id="@+id/thiamaikbutton"
        android:layout_width="360dp"
        android:layout_height="52dp"
        android:layout_marginTop="10dp"
        android:text="GOH THIAM AIK"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/zoiebutton" />

    <Button
        android:id="@+id/marhakimbutton"
        android:layout_width="360dp"
        android:layout_height="52dp"
        android:layout_marginTop="10dp"
        android:text="MUHAMMAD MARHAKIM BIN MARZUKI"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/thiamaikbutton" />

    <Button
        android:id="@+id/peishanbutton"
        android:layout_width="360dp"
        android:layout_height="52dp"
        android:layout_marginTop="10dp"
        android:text="LI PEISHAN"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/marhakimbutton" />

    <Button
        android:id="@+id/claudiabutton"
        android:layout_width="360dp"
        android:layout_height="52dp"
        android:layout_marginTop="10dp"
        android:text="CLAUDIA CHEE XIN LIN"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/peishanbutton" />

    <TextView
        android:id="@+id/output"
        android:layout_width="360dp"
        android:layout_height="250dp"
        android:layout_marginTop="20dp"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/claudiabutton" />

</androidx.constraintlayout.widget.ConstraintLayout>